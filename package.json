{
  "name": "stol",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint --ext .ts,.tsx . --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e:start": "cypress open",
    "test:e2e": "cypress run",
    "deploy": "aws --endpoint-url=https://storage.yandexcloud.net s3 cp --recursive dist/ s3://stol.zhilin.app/",
    "deploy:preview": "aws --endpoint-url=https://storage.yandexcloud.net s3 cp --recursive dist/ s3://preview.stol.zhilin.app/",
    "prepare": "husky install"
  },
  "dependencies": {
    "@atlaskit/atlassian-navigation": "^2.2.3",
    "@atlaskit/avatar": "^21.0.0",
    "@atlaskit/button": "^16.3.2",
    "@atlaskit/calendar": "^12.2.0",
    "@atlaskit/icon": "^21.10.7",
    "@atlaskit/menu": "^1.3.6",
    "@atlaskit/page-header": "^10.2.3",
    "@atlaskit/page-layout": "^1.2.5",
    "@atlaskit/popper": "^5.2.5",
    "@atlaskit/spinner": "^15.1.12",
    "@atlaskit/toggle": "^12.4.8",
    "@editorjs/checklist": "^1.3.0",
    "@editorjs/editorjs": "^2.24.3",
    "@editorjs/image": "^2.6.2",
    "@editorjs/link": "^2.4.1",
    "@editorjs/paragraph": "^2.8.0",
    "@reduxjs/toolkit": "^1.8.2",
    "dayjs": "^1.11.3",
    "editorjs-strikethrough": "^1.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@honkhonk/vite-plugin-svgr": "^1.1.0",
    "@testing-library/react": "^13.3.0",
    "@types/node": "^17.0.40",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.7",
    "cypress": "^10.0.3",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^8.0.1",
    "jsdom": "^19.0.0",
    "lint-staged": ">=10",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3",
    "vite": "^2.9.9",
    "vite-plugin-mkcert": "^1.7.1",
    "vitest": "^0.14.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix --max-warnings 0",
    "*.{js,jsx,ts,tsx,css,md,json}": "prettier --write"
  }
}
